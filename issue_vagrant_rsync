        if Vagrant::Util::Platform.windows?
          # rsync for Windows expects cygwin style paths, always.
          #hostpath = Vagrant::Util::Platform.cygwin_path(hostpath)
		  hostpath = "/cygdrive" + Vagrant::Util::Platform.cygwin_path(hostpath)
        end

